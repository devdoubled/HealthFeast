{
  "expo": {
    "name": "HealthFeast",
    "slug": "healthfeast-mobile",
    "version": "1.0.0",
    "scheme": "com.devdd.healthfeast",
    "orientation": "portrait",
    "icon": "./src/assets/images/sub_logo.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/sub_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devdd.healthfeast",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {},
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fa441770-aa12-4515-84a0-413f3fea6555"
      }
    }
  }
}
